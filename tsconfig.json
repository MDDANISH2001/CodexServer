{
  "compilerOptions": {
    "target": "es2019",
    "module": "NodeNext",
    "moduleResolution": "NodeNext", 
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*", "src/helper/userpipeline.ts"],
  "exclude": ["node_modules"]

}